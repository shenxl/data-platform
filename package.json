{
  "name": "dataPlatform",
  "version": "1.0.0",
  "description": "数据统计服务",
  "private": true,
  "dependencies": {
    "egg": "^1.7.0",
    "egg-mysql": "^3.0.0",
    "egg-passport": "^1.0.0",
    "egg-scripts": "^1.1.1",
    "egg-sequelize": "^3.1.0",
    "mysql2": "^1.4.2",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "autod": "^2.9.0",
    "autod-egg": "^1.0.0",
    "babel-eslint": "6",
    "co": "^4.6.0",
    "egg-bin": "^4.2.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.10.0",
    "eslint": "3",
    "eslint-config-egg": "3",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
